{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:ListBucket",
        "s3:DeleteObject"
      ],
      "Resource": [
        "arn:aws:s3:::speakeasy-conversations",
        "arn:aws:s3:::speakeasy-conversations/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "rekognition:DetectFaces",
        "transcribe:StartStreamTranscription",
        "bedrock:InvokeModel"
      ],
      "Resource": "*"
    }
  ]
}
